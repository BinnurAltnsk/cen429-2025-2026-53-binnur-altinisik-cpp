          <type>int32_t</type>
          <declname>tripId</declname>
        </param>
        <param>
          <type><ref refid="namespace_travel_expense_1a3c96c6d4d2d9f4441ba6d1d2d3124b62" kindref="member">ExpenseCategory</ref></type>
